package estrutura_repeticao;

import java.util.Scanner;

public class Exe_05_curso {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		
		int N = sc.nextInt();
		
		for(int i = 0; i < N; i++) {
			
			int calc = N * (N - 1) * (N - 2) * (N - 3) * 1;
		}
		
		System.out.println(calc);
		
		sc.close();

	}

}
